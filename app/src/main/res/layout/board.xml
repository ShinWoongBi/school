                        android:text="432"
                        android:textSize="17sp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/title6"
                        android:paddingLeft="10dp"
                        android:singleLine="true"
                        android:textSize="20sp"
